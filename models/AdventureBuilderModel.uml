      <packagedElement xmi:type="uml:Dependency" xmi:id="_eMTmsKHYEeuolPkAC7VaLg" client="_I3j2sKHYEeuolPkAC7VaLg" supplier="_NR9lcKHYEeuolPkAC7VaLg"/>
